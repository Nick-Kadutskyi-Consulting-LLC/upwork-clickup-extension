{
  "manifest_version": 2,
  "default_locale": "en",
  "name": "Upwork to ClickUp",
  "description": "Send Upwork leads to ClickUp",
  "version": "0.1",
  "icons": {
    "48": "images/icon-48.png",
    "96": "images/icon-96.png",
    "128": "images/icon-128.png",
    "256": "images/icon-256.png",
    "512": "images/icon-512.png"
  },
  "background": {
    "page": "dist/background.html",
    "persistent": false
  },
  "content_scripts": [
    {
      "js": [
        "browser-polyfill.min.js",
        "dist/content.js"
      ],
      "matches": [
        "*://*.upwork.com/*"
      ]
    }
  ],
  "options_ui": {
    "page": "dist/index.html",
    "browser_style": true
  },
  "browser_action": {
    "default_icon": {
      "48": "images/disabled-48x48@1x.png"
    }
  },
  "permissions": [
    "storage",
    "https://api.clickup.com/*",
    "contextMenus"
  ],
  "web_accessible_resources": [
    "images/cl-icon.png"
  ]
}
